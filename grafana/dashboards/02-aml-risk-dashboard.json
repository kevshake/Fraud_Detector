{
  "dashboard": {
    "title": "AML/Fraud Detector - AML Risk Dashboard",
    "tags": ["aml", "risk", "compliance"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "AML Risk Assessments (Rate)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(aml_risk_assessments_total[5m])",
            "legendFormat": "Total Assessments",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Assessments/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Risk Level Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(aml_risk_high_total[5m]))",
            "legendFormat": "High Risk",
            "refId": "A"
          },
          {
            "expr": "sum(rate(aml_risk_medium_total[5m]))",
            "legendFormat": "Medium Risk",
            "refId": "B"
          },
          {
            "expr": "sum(rate(aml_risk_low_total[5m]))",
            "legendFormat": "Low Risk",
            "refId": "C"
          }
        ]
      },
      {
        "id": 3,
        "title": "AML Assessment Time (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(aml_assessment_time_bucket[5m])) by (le))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(aml_assessment_time_bucket[5m])) by (le))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(aml_assessment_time_bucket[5m])) by (le))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Assessment Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Active AML Cases",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "aml_active_cases",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Risk Detection Types",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(aml_velocity_checks_total[5m]))",
            "legendFormat": "Velocity Checks",
            "refId": "A"
          },
          {
            "expr": "sum(rate(aml_geographic_risk_total[5m]))",
            "legendFormat": "Geographic Risk",
            "refId": "B"
          },
          {
            "expr": "sum(rate(aml_pattern_risk_total[5m]))",
            "legendFormat": "Pattern Risk",
            "refId": "C"
          },
          {
            "expr": "sum(rate(aml_amount_risk_total[5m]))",
            "legendFormat": "Amount Risk",
            "refId": "D"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Detections/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Risk Level Trends (Last 24h)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(aml_risk_high_total[5m]))",
            "legendFormat": "High Risk",
            "refId": "A"
          },
          {
            "expr": "sum(rate(aml_risk_medium_total[5m]))",
            "legendFormat": "Medium Risk",
            "refId": "B"
          },
          {
            "expr": "sum(rate(aml_risk_low_total[5m]))",
            "legendFormat": "Low Risk",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Risk Assessments/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Risk Assessment Success Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(aml_risk_assessments_total[5m])) / sum(rate(aml_transactions_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}

