{
  "dashboard": {
    "title": "AML/Fraud Detector - Transaction Overview",
    "tags": ["aml", "transactions", "overview"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Total Transactions (Rate)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(aml_transactions_total[5m])",
            "legendFormat": "{{merchant_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Transactions/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 2,
        "title": "Transaction Actions Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(aml_transactions_blocked_total[5m])) by (reason)",
            "legendFormat": "Blocked - {{reason}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(aml_transactions_allowed_total[5m]))",
            "legendFormat": "Allowed",
            "refId": "B"
          },
          {
            "expr": "sum(rate(aml_transactions_held_total[5m]))",
            "legendFormat": "Held",
            "refId": "C"
          },
          {
            "expr": "sum(rate(aml_transactions_alerted_total[5m]))",
            "legendFormat": "Alerted",
            "refId": "D"
          }
        ]
      },
      {
        "id": 3,
        "title": "Transaction Processing Time (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(aml_transactions_processing_time_bucket[5m])) by (le))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(aml_transactions_processing_time_bucket[5m])) by (le))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(aml_transactions_processing_time_bucket[5m])) by (le))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Processing Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Transactions by Merchant (Top 10)",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "topk(10, sum(rate(aml_transactions_by_merchant_total[5m])) by (merchant_id))",
            "legendFormat": "{{merchant_id}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 5,
        "title": "Transaction Queue Size",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "aml_transactions_queue_size",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Transactions by Risk Level",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(aml_transactions_by_risk_level_total[5m])) by (risk_level)",
            "legendFormat": "{{risk_level}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Transactions/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Transaction Processing Rate (Last Hour)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "rate(aml_transactions_processed_total[1m])",
            "legendFormat": "Processed",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Transactions/sec"},
          {"format": "short"}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}

