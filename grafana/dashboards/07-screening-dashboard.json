{
  "dashboard": {
    "title": "AML/Fraud Detector - Screening Dashboard",
    "tags": ["screening", "sanctions", "compliance"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All PSPs",
            "value": "$__all"
          },
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "definition": "label_values(psp_code)",
          "hide": 0,
          "includeAll": true,
          "multi": true,
          "name": "PSP",
          "options": [],
          "query": {
            "query": "label_values(psp_code)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "type": "query"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Screening Operations (Rate)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(screening_total{psp_code=~\"$PSP\"}[5m])",
            "legendFormat": "Total Screenings",
            "refId": "A"
          },
          {
            "expr": "rate(screening_matches_total{psp_code=~\"$PSP\"}[5m])",
            "legendFormat": "Matches",
            "refId": "B"
          },
          {
            "expr": "rate(screening_no_matches_total{psp_code=~\"$PSP\"}[5m])",
            "legendFormat": "No Matches",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Screenings/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Match Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(screening_matches_total{psp_code=~\"$PSP\"}[5m])) / sum(rate(screening_total{psp_code=~\"$PSP\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Screening Queue Size",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(screening_queue_size{psp_code=~\"$PSP\"})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Screening by Type",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(screening_by_type_total{psp_code=~\"$PSP\"}[5m])) by (type, psp_code)",
            "legendFormat": "{{type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Screenings/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Screening Time (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(screening_time_bucket{psp_code=~\"$PSP\"}[5m])) by (le, psp_code))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(screening_time_bucket{psp_code=~\"$PSP\"}[5m])) by (le, psp_code))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(screening_time_bucket{psp_code=~\"$PSP\"}[5m])) by (le, psp_code))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Screening Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Screening Match Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(screening_matches_total{psp_code=~\"$PSP\"}[5m]))",
            "legendFormat": "Matches",
            "refId": "A"
          },
          {
            "expr": "sum(rate(screening_no_matches_total{psp_code=~\"$PSP\"}[5m]))",
            "legendFormat": "No Matches",
            "refId": "B"
          }
        ]
      },
      {
        "id": 7,
        "title": "Screening Throughput (Last 24h)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(screening_total{psp_code=~\"$PSP\"}[1m])",
            "legendFormat": "Screenings/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Throughput"},
          {"format": "short"}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}

